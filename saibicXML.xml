<?xml version="1.0" encoding="utf-8"?>
<!-- https://github.com/HonzaSaibic/JazykySemestralniProjekt/blob/master/saibicXML.xml -->
<gameSettings>
 <!-- System Settings -->
  <system>
    <graphics>
      <resolution>1920x1080</resolution>
      <verticalSync>1</verticalSync>
      <graphicsQuality>6</graphicsQuality>
      <viewDistance>6</viewDistance>
      <environmentDetail>6</environmentDetail>
      <groundClutter>6</groundClutter>
      <shadowQuality>high</shadowQuality>
      <liquidDetail>good</liquidDetail>
      <lightningQuality>high</lightningQuality>
      <textureResolution>high</textureResolution>
    </graphics>
    <advanced>
      <trippleBuffering>0</trippleBuffering>
      <reduceInputLag>0</reduceInputLag>
      <maxForegroundFps>100</maxForegroundFps>
      <maxBackgroundFps>30</maxBackgroundFps>
      <uiScale>100%</uiScale>
    </advanced>
    <network>
      <optimizeNetworkForSpeed>1</optimizeNetworkForSpeed>
      <enableIpv6WhenAvailable>0</enableIpv6WhenAvailable>
      <advancedCombatLogging>0</advancedCombatLogging>
    </network>
    <languages>
      <text>English</text>
      <audio>English</audio>
    </languages>
    <sound>
      <masterVolume>21%</masterVolume>
      <music>50%</music>
      <ambience>50%</ambience>  
      <dialog>60%</dialog>
      <soundInBackground>0</soundInBackground>
      <distanceFiltering>0</distanceFiltering>
      <emoteSounds>1</emoteSounds>
      <errorSpeech>0</errorSpeech>
    </sound>
  </system>
  
<!-- AddOns -->
  <addOns>
    <addon addonName="DoomCD">
      <active>0</active>
    </addon>
    <addon addonName="bagAddon">
      <active>1</active>
    </addon>
    <addon addonName="superCoolAddon">
      <active>1</active>
    </addon>
  </addOns>
  
